{
  "name": "Revolut_Assignment",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Yi-Shiuan Chen, yschen25@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build --historyApiFallback --config webpack.dev.config.js",
    "prod": "webpack --config webpack.prod.config.js",
    "analyze": "webpack --config webpack.prod.config.js --profile --json > stats.json",
    "report": "webpack-bundle-analyzer ./stats.json",
    "lint": "eslint src/*.js",
    "lint-fix": "eslint --fix src/*.js",
    "test": "jest --coverage",
    "test:single": "node ./node_modules/jest-single-file-coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "jest": {
    "setupFiles": [
      "@testing-library/react/dont-cleanup-after-each",
      "./__test__/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__test__/__mocks__/fileTransformer.js"
    },
    "collectCoverageFrom": [
      "src/styles/*.js",
      "src/commons/*.js",
      "src/containers/*/*.js",
      "src/routes/home/*/*.js",
      "!src/assets/*.*"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@react-icons/all-files": "^4.1.0",
    "acorn": "^6.4.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "connected-react-router": "^6.8.0",
    "file-loader": "^5.1.0",
    "happypack": "^5.0.1",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "kind-of": "^6.0.3",
    "minimist": "^0.2.1",
    "prop-types": "15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "styled-components": "^4.4.1",
    "styled-reset": "^4.1.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-single-file-coverage": "^1.0.2",
    "jest-styled-components": "^6.3.4",
    "jest-transform-stub": "^2.0.0",
    "path": "^0.12.7"
  }
}
