{
  "name": "Revolut_Assignment",
  "version": "1.0.0",
  "main": "main.js",
  "author": "Yi-Shiuan Chen, yschen25@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --devtool eval --progress --hot true --config webpack.dev.config.js",
    "prod": "webpack --config webpack.prod.config.js",
    "analyze": "webpack --config webpack.prod.config.js --profile --json > stats.json",
    "report": "webpack-bundle-analyzer ./stats.json",
    "test": "jest --coverage",
    "test:single": "node ./node_modules/jest-single-file-coverage"
  },
  "jest": {
    "setupFiles": [
      "@testing-library/react/dont-cleanup-after-each",
      "./__test__/setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__test__/__mocks__/fileTransformer.js"
    },
    "collectCoverageFrom": [
      "src/components/*/*.tsx"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@hot-loader/react-dom": "^16.13.0",
    "@react-icons/all-files": "^4.1.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "acorn": "^6.4.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "connected-react-router": "^6.9.1",
    "currencies.json": "^1.0.2",
    "file-loader": "^5.1.0",
    "happypack": "^5.0.1",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "jsonp": "^0.2.1",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.21",
    "minimist": "^0.2.1",
    "prop-types": "15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "styled-components": "^4.4.1",
    "styled-reset": "^4.1.2",
    "terser-webpack-plugin": "^2.3.5",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/styled-components": "^5.1.15",
    "@types/webpack-env": "^1.16.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-single-file-coverage": "^1.0.2",
    "jest-styled-components": "^6.3.4",
    "jest-transform-stub": "^2.0.0",
    "path": "^0.12.7",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0"
  }
}
